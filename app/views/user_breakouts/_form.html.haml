= form_for @user_breakout do |f|
  - if @user_breakout.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_breakout.errors.count, "error")} prohibited this user_breakout from being saved:"
      %ul
        - @user_breakout.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :breakout_id
    = f.number_field :breakout_id
  .actions
    = f.submit 'Save'
